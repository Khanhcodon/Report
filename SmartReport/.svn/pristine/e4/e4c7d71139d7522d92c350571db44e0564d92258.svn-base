
namespace Bkav.eGovCloud.Entities.Customer
{
    /// <summary>
    /// Giấy tờ bổ sung đăng ký qua mạng
    /// </summary>
    public class OnlinePaper
    {
        /// <summary>
        /// Lấy hoặc thiết lập Id giấy tờ
        /// </summary>
        public int PaperId { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập Tên giấy tờ
        /// </summary>
        public string PaperName { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập Số lượng
        /// </summary>
        public int Amount { get; set; }

        /// <summary>
        /// Thiết lập trạng thái đã bổ sung
        /// </summary>
        public bool? IsSupplementaried { get; set; }
    }
}
