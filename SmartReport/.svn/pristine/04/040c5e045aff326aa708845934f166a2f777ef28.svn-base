.toolBar {
	vertical-align: top;
	display: inline-block;
	white-space: nowrap;
}

	.toolBar .toolBarUL {
		vertical-align: top;
		display: inline-block;
		white-space: nowrap;
	}

	.toolBar .toolBarItem {
		vertical-align: top;
		display: inline-block;
		white-space: nowrap;
	}

	.toolBar .toolBarWrap {
		vertical-align: top;
		display: inline-block;
		white-space: nowrap;
	}

	.toolBar .toolBarOut {
		vertical-align: top;
		display: inline-block;
		white-space: nowrap;
	}

	.toolBar .toolBarIn {
		vertical-align: top;
		display: inline-block;
		white-space: nowrap;
	}

.toolBar {
	margin: 0px;
	padding: 0px;
	text-align: left;
	cursor: default;
}

	.toolBar .toolBarUL {
		margin: 0px;
		padding: 0px;
		vertical-align: middle;
		list-style-type: none;
		list-style-position: outside;
	}

	.toolBar .toolBarItem {
		margin: 0px;
		padding: 0px;
		vertical-align: middle;
		list-style-type: none;
		list-style-position: outside;
	}

	.toolBar .toolBarItem {
		margin: 0px 1px;
	}

.toolBarVertical .toolBarItem {
	margin: 0px;
	display: block;
}

.toolBar .toolBarWrap {
	outline: 0px;
	text-decoration: none;
	vertical-align: top;
	cursor: pointer;
}

	.toolBar .toolBarWrap:visited {
		color: inherit;
	}

	.toolBar .toolBarWrap:hover {
		color: inherit;
	}

	.toolBar .toolBarWrap:active {
		color: inherit;
	}

.toolBar .toolBarVOriented {
	text-align: center;
}

.toolBar .toolBarIn {
	vertical-align: middle;
}

	.toolBar .toolBarIn * {
		vertical-align: middle;
	}

.toolBar .toolBarIn {
	padding: 3px 0px 0px 3px;
	border: 1px solid rgb(255, 255, 255);
	border-image: none;
	overflow: hidden;
	cursor: pointer;
}

.toolBar .toolBarHover {
	border-color: rgb(238, 238, 238) rgb(170, 170, 170) rgb(170, 170, 170) rgb(238, 238, 238);
}

