INSERT INTO `report` VALUES ('195', null, 'Sổ văn bản đi', '0', '[9,21,22,23]', 'Select DATE_FORMAT(dp.DatePublished,\'%d/%m/%Y\') as DatePublish, d.DocCode, d.Compendium, us.FullName as UserSuccess, d.TotalPage, \r\ndp.AddressName as Address, d.InOutPlace as PrivatePublishPlace, d.Organization, DATE_FORMAT(dp.DateAppointed, \'%d/%m/%Y\') as DateAppointed,\r\nsd.StoreId, st.StoreName\r\nFROM document d\r\nJOIN documentcopy dc on dc.DocumentId = d.DocumentId\r\nJOIN store_doc sd on sd.DocumentId = d.DocumentId\r\nJOIN store st on st.StoreId = sd.StoreId\r\nJOIN category on category.CategoryId = d.CategoryId\r\nLEFT Join urgent ug on ug.UrgentId = d.UrgentId\r\nLEFT JOIN `user` us on us.UserId = d.UserSuccessId\r\nLEFT JOIN doc_publish dp on dp.DocumentCopyId = dc.DocumentCopyId\r\nWHERE dc.DocumentCopyType = 1\r\n	AND d.`Status` NOT IN (1, 8)\r\n	AND d.CategoryBusinessId = 2\r\n	AND TIMEDIFF(d.DateCreated, @from) >= 0 \r\n	AND TIMEDIFF(d.DateCreated, @to) <= 0\r\n  AND (@treeGroupValue IS NULL OR  @treeGroupValue=\'\' or #treeGroup = @treeGroupValue)\r\nORDER BY dp.DatePublished\r\n', '[19]', 'Select DATE_FORMAT(dp.DatePublished,\'%d/%m/%Y\') as DatePublish, d.DocCode, d.Compendium, us.FullName as UserSuccess, d.TotalPage, \r\ndp.AddressName as Address, d.InOutPlace as PrivatePublishPlace, d.Organization, DATE_FORMAT(dp.DateAppointed, \'%d/%m/%Y\') as DateAppointed,\r\nsd.StoreId, st.StoreName\r\nFROM document d\r\nJOIN documentcopy dc on dc.DocumentId = d.DocumentId\r\nJOIN store_doc sd on sd.DocumentId = d.DocumentId\r\nJOIN store st on st.StoreId = sd.StoreId\r\nJOIN category on category.CategoryId = d.CategoryId\r\nLEFT Join urgent ug on ug.UrgentId = d.UrgentId\r\nLEFT JOIN `user` us on us.UserId = d.UserSuccessId\r\nLEFT JOIN doc_publish dp on dp.DocumentCopyId = dc.DocumentCopyId\r\nWHERE dc.DocumentCopyType = 1\r\n	AND d.`Status` NOT IN (1, 8)\r\n	AND d.CategoryBusinessId = 2\r\n	AND TIMEDIFF(d.DateCreated, @from) >= 0 \r\n	AND TIMEDIFF(d.DateCreated, @to) <= 0\r\n	AND (@groupValue = \'\' or #groupValue = @groupValue)\r\n       AND (@treeGroupValue IS NULL OR  @treeGroupValue=\'\' or #treeGroup = @treeGroupValue)\r\nORDER BY dp.DatePublished\r\nLIMIT @skip, @take', 'SELECT \r\n	#groupValue as GroupValue,\r\n	#groupName as GroupName,\r\n	COUNT(#groupValue) as Total\r\nFROM document d\r\nJOIN documentcopy dc on dc.DocumentId = d.DocumentId\r\nJOIN store_doc sd on sd.DocumentId = d.DocumentId\r\nJOIN store st on st.StoreId = sd.StoreId\r\nLEFT Join urgent ug on ug.UrgentId = d.UrgentId\r\nJoin category on d.CategoryId = category.CategoryId\r\nLEFT JOIN `user` us on us.UserId = d.UserSuccessId\r\nWHERE \r\n	`d`.`Status` NOT IN (1,8)\r\n	AND `sd`.`StoreId` = @treeGroupValue\r\n	and dc.DocumentCopyType = 1\r\n	and d.CategoryBusinessId = 2\r\n	AND DATEDIFF(d.DateCreated, @from) >= 0 \r\n	AND DATEDIFF(d.DateCreated, @to) <= 0\r\n	AND (@treeGroupValue = \'\' or #treeGroup = @treeGroupValue)\r\ngroup by #groupValue', 'Select Count(1)\r\nFROM document d\r\nJOIN documentcopy dc on dc.DocumentId = d.DocumentId\r\nJOIN store_doc sd on sd.DocumentId = d.DocumentId\r\nJOIN store st on st.StoreId = sd.StoreId\r\nWHERE \r\n	`d`.`Status` NOT IN (1,8)\r\n	AND `sd`.`StoreId` = @treeGroupValue\r\n	AND d.CategoryBusinessId = 2\r\n	and dc.DocumentCopyType = 1\r\n	AND DATEDIFF(d.DateCreated, @from) >= 0 \r\n	AND DATEDIFF(d.DateCreated, @to) <= 0', null, '{\"Fields\":[{\"Name\":\"Số ký hiệu\",\"Key\":\"DocCode\",\"Type\":1,\"DataType\":\"string\"},{\"Name\":\"Trích Yếu\",\"Key\":\"Compendium\",\"Type\":1,\"DataType\":\"string\"},{\"Name\":\"ĐV Soạn thảo\",\"Key\":\"InOutPlace\",\"Type\":1,\"DataType\":\"string\"},{\"Name\":\"Người ký\",\"Key\":\"UserSuccess\",\"Type\":1,\"DataType\":\"string\"},{\"Name\":\"Nơi nhận\",\"Key\":\"Address\",\"Type\":1,\"DataType\":\"string\"},{\"Name\":\"Ngày ban hành\",\"Key\":\"DatePublish\",\"Type\":1,\"DataType\":\"string\"},{\"Name\":\"Số bản\",\"Key\":\"TotalPage\",\"Type\":1,\"DataType\":\"string\"},{\"Name\"