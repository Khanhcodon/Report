<div class="codeTempDetail">
    <table class="table table-bordered table-striped" id="codeTempDetail">
        <colgroup>
            <col style="width: 250px;">
            <col />
            <col style="width:250px;" />
            <col style="width:50px;"/>
        </colgroup>

        <thead>
            <tr>
                <th class="text-center" style="padding-bottom:10px;">Mã</th>
                <th class="text-center" style="padding-bottom:10px;">Người cấp</th>
                <th class="text-center" style="padding-bottom:10px;">Ngày tạo</th>
                <td class="text-center">Xóa</td>
            </tr>
        </thead>
        <tbody>
            {{each templateCode}}
            <tr>
                <td class="text-center border-right">${Code}</td>
                <td class="text-center border-right">${FullName}</td>
                <td class="text-center border-right">${DateCreated}</td>
                <td class="text-center">
                    <a href="#" class="btn btn-xs btn-danger btn-DeleteCode" id="${CodeTempId}" style="margin:3px 0">Xóa</a>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
