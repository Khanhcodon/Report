DROP TABLE IF EXISTS `reportmodes`;
CREATE TABLE `reportmodes`  (
   `ReportModeId` int(11) NOT NULL AUTO_INCREMENT,
   `Code` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
   `Name` varchar(500) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
   `Subject` varchar(500) CHARACTER SET utf8 COLLATE utf8_unicode_ci  NULL,
   `IssueOrg` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
   `IssueDate` datetime(0) NULL DEFAULT NULL,
   `Number` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NULL,
   `Notation` varchar(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NULL,
   `RefNotation` MEDIUMTEXT CHARACTER SET utf8 COLLATE utf8_unicode_ci NULL,
   `ReportMode` int(11) NOT NULL,
   `Attachments` MEDIUMTEXT CHARACTER SET utf8 COLLATE utf8_unicode_ci NULL,
  PRIMARY KEY (`ReportModeId`) USING BTREE
) ENGINE = InnoDB CHARACTER SET = utf8 COLLATE = utf8_unicode_ci 