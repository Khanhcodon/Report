@model CatalogModel
@{
    ViewBag.Title = Localizer("Common.Catalog.Label.Create");
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
    function ToIndex() {
        document.location = "../../../admin/Catalog";
    }

    $(function () {
        $("#CatalogName").val("");
    });
</script>
<div class="section-header">
    <div class="title">
        <i class="fa icon-popup"></i>
        <span data-res="egov.resources.sitemap.catalog"></span>
    </div>
    <div class="options">
        <a class="btn  btn-warning control-btn" id="submit" href="#">
            <span class="icon icon-checkmark"></span>
            <span class="text-uppercase" data-res="egov.resources.buttons.save"></span>
        </a>
        <a class="btn  btn-default control-btn" href="/admin/catalog/Index" id="back">
            <span class="icon icon-arrow-left2"></span>
            <span class="text-uppercase" data-res="egov.resources.buttons.back"></span>
        </a>
    </div>
</div>

@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.CatalogId)
    <div id="tabs" class="bkavegov">
          @Html.Partial("_Notifications")
        @Html.Partial("_CreateOrEdit", Model)
        @Html.AntiForgeryToken()
    </div>
}
