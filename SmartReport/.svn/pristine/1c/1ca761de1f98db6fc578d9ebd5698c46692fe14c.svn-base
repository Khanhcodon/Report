@import url('../font-awesome-4.7.0/css/font-awesome.min.css');

@font-face {
    font-family: 'Roboto-BoldItalic';
    src: url('../fonts/Roboto-BoldItalic.ttf') format('truetype');
}

@font-face {
    font-family: 'Roboto-Bold';
    src: url('../fonts/Roboto-Bold.ttf') format('truetype');
}

li, a, span {
    cursor: pointer !important;
}

#content-agv {
    background-image: url(../../Content/Images/home/background-web2.png);
    background-size: 100%;
}

#menu-document {
    font-family: 'Roboto', sans-serif;
}

    #menu-document > li > ul {
        margin-top: 10px;
    }

    #menu-document > li > a {
        padding: 15px 25px 15px 5px !important;
        margin-top: 0 !important;
        display: block;
        font-size: 13px !important;
        font-weight: 700 !important;
        color: #e8e4e2 !important;
        border-left: 3px solid rgba(255,255,255,0.00);
    }

        #menu-document > li > a > .fa {
            width: 40px;
            font-size: 18px;
            display: inline-block;
            vertical-align: middle;
            padding-left: 0;
            margin-right: 0px;
            line-height: 40px;
            text-align: center;
            background-color: rgba(0, 0, 0, 0);
            border-radius: 0px;
        }

        #menu-document > li > a > .pull-right-container {
            position: absolute;
            right: 18px;
            width: 20px;
            height: 18px;
        }

    #menu-document > li > ul > li { /*border-bottom: 0.5px solid #e8e4e2;*/
        /*cursor: pointer !important;
        padding: 5px 0 0 0;*/
    }

    #menu-document li ul li a {
        padding: unset;
        display: block;
        font-size: 15px;
        font-weight: 400;
        color: #ffffff;
        clear: both;
        height: 40px;
        vertical-align: middle;
        /*line-height: 40px;*/
        padding-left: 12px;
    }

    #menu-document > li > ul > li a:hover {
        font-weight: 700 !important;
        cursor: pointer !important;
    }

    #menu-document > li > ul > li > a.list-group-item.active, #menu-document > li > ul > li > a.list-group-item.active:hover, #menu-document > li > ul > li > a.list-group-item.active:focus {
        background: transparent !important;
        font-weight: 700 !important;
        cursor: pointer !important;
    }
     
    #menu-document > li > ul > li > ul > li > a.list-group-item.active,
    #menu-document > li > ul > li > ul > li > a.list-group-item.active:hover,
    #menu-document > li > ul > li > ul > li > a.list-group-item.active:focus {
        background: transparent !important;
        font-weight: 700 !important;
        cursor: pointer !important;   
    }

    #menu-document > li > ul > li > ul > li >  ul > li > a.list-group-item.active,
    #menu-document > li > ul > li > ul > li >  ul > li > a.list-group-item.active:hover,
    #menu-document > li > ul > li > ul > li >  ul > li > a.list-group-item.active:focus {
        background: transparent !important;
        font-weight: 700 !important;
        cursor: pointer !important;   
    }

    #menu-document > li > ul > li > ul > li >  ul > li >  ul > li > a.list-group-item.active,
    #menu-document > li > ul > li > ul > li >  ul > li >  ul > li > a.list-group-item.active:hover,
    #menu-document > li > ul > li > ul > li >  ul > li >  ul > li > a.list-group-item.active:focus {
        background: transparent !important;
        font-weight: 700 !important;
        cursor: pointer !important;   
    }



    #menu-document > li.menu-open > a > .pull-right-container > .fa-angle-right {
        -webkit-transform: rotate(90deg);
        -ms-transform: rotate(90deg);
        -o-transform: rotate(90deg);
        transform: rotate(90deg);
    }

    #menu-document > li:hover > a, #menu-document > li.menu-open > a {
        color: #e8e4e2;
        background: #5a240c;
    }

    #menu-document ul {
        clear: both;
    }

    #menu-document a {
        font-size: 16px !important;
    }

.nav#menu-document li {
    clear: both;
}

ul li .node-name {
    max-width: calc(100% - 50px);
    float: left;
    word-break: keep-all;
}
