}

    ul#notificationCenter > li.user-option > a > img {
        width: 25px !important;
        height: 25px;
        margin-left: 0 !important;
    }


    ul#notificationCenter > li.user-option > a.user-avt {
        padding-left: 0 !important;
    }

ul#notificationCenter > li.nav-setting {
