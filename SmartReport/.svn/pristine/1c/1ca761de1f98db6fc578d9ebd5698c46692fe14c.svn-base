.site-menu div.menu-items > a, .site-menu .setting-items, .site-menu .setting-items > a, .site-menu div.menu-items > a.active::after {
    background-color: unset !important;
}

.site-menu .list-group-item {
    margin-bottom: unset;
    padding: 25px 5px 25px 5px;
    background-repeat: no-repeat !important;
    background-position: center !important;
}

    .site-menu .list-group-item img {
        margin-right: unset;
        width: unset;
        height: unset;
    }

.panel-container .west-panel {
    width: 50px !important;
}

.btnToggleTree {
    background: url(../../Content/Images/home/ic_chevron_right.png) no-repeat !important;
}

ul.nav.tree-view-el li > a > .menu-icon.closed {
    background: url(../../Content/Images/home/ic_chevron_right.png) no-repeat !important;
    transform: rotate(-90deg);
    margin-top: 5px;
}

ul.nav.tree-view-el li > a > .menu-icon.open {
    background: url(../../Content/Images/home/ic_chevron_right.png) no-repeat !important;
    /*margin-top: 8px;*/
}

.setting-items .list-group-item {
}

.search-nav .btn-search {
    border: none;
    width: 25px;
    height: 100%;
    background-image: url(../../Content/Images/home/filter.png);
    background-repeat: no-repeat;
    text-align: center;
    background-position: center;
    outline: none;
    cursor: pointer;
    background-color: transparent;
}

.pull-left ul.grp-search {
    margin: 15px 0 0 10px;
    height: 32px;
    padding: 0 5px;
    border-radius: 5px;
    border: 1px solid #ECECEC;
    float: left;
}

    .pull-left ul.grp-search li {
        float: left;
        text-decoration: none;
        list-style: none;
        vertical-align: middle;
        line-height: 30px;
    }

        .pull-left ul.grp-search li input.search-query {
            border: none;
            height: 30px;
            background-color: #F8F8F8;
        }

.pull-right ul {
    margin: 15px 0 0 10px;
    height: 32px;
}

    .pull-right ul.create-report li {
        float: left;
        text-decoration: none;
        list-style: none;
        padding-right: 15px;
        height: 30px;
    }

ul.create-report li button {
    outline: none;
    cursor: pointer !important;
    background-color: #F39C1A;
    color: #ffffff;
    border: none;
}

    ul.create-report li button:focus {
        outline: 0 !important;
        color: #ffffff !important;
    }

    /*ul.create-report li:hover {
            cursor: pointer;
            color: #ffffff !important;
        }*/
    ul.create-report li button:hover {
        cursor: pointer !important;
        color: #ffffff !important;
    }

.site-menu .list-group-item.dashboard {
    background-image: url(/Content/Images/home/ic_account_balance_24px.png);
}

    .site-menu .list-group-item.dashboard.active, .site-menu .list-group-item.dashboard:hover {
        background-image: url(/Content/Images/home/ic_account_balance.png);
    }

.site-menu .list-group-item.egov.documents {
    background-image: url(/Content/Images/home/ic_copy-inactive.png);
}

    .site-menu .list-group-item.egov.documents.active, .site-menu .list-group-item.egov.documents:hover {
        background-image: url(/Content/Images/home/ic_copy.png);
    }

.site-menu .list-group-item.report {
    background-image: url(/Content/Images/home/Path.png);
}

.site-menu .list-group-item.search {
    background-image: url(/Content/Images/home/ic_zoom.png);
}

.site-menu .list-group-item.search.active, .site-menu .list-group-item.search:hover{
    background-image: url(/Content/Images/home/ic_zoom.png);
}

.site-menu .list-group-item.report.active, .site-menu .list-group-item.report:hover {
    background-image: url(/Content/Images/home/Path-active.png);
}

.list-group.setting-items #resetSystem {
    background-image: url(/Content/Images/home/ic_settings_backup_restore.png);
}

    .list-group.setting-items #resetSystem.active, .list-group.setting-items #resetSystem:hover {
        background-image: url(/Content/Images/home/ic_settings_backup_restore-active.png);
    }

.list-group.setting-items #setting-dialog {
    background-image: url(/Content/Images/home/setting.png);
}

#setting-dialog.active, #setting-dialog:hover {
    background-image: url(/Content/Images/home/setting-active.png);
}

.list-group.setting-items #logout {
    background-image: url(/Content/Images/home/shutdown.png);
}

    .list-group.setting-items #logout.active, .list-group.setting-items #logout:hover {
        background-image: url(/Content/Images/home/shutdown-active.png);
    }

