<form class="form-horizontal search-form" id="formImportInvoice">
    <div class="col-md-16">
        <div class="form-group form-group-sm" style="margin-bottom:7px">
            <label class="col-md-4" data-res="">Mã biên lai</label>
            <div class="col-md-12">
                <input type="text" name="key" value="${key}" class="form-control" readonly />
            </div>
        </div>
        <div class="form-group form-group-sm" style="margin-bottom:7px">
            <label class="col-md-4" data-res="">Tên Công dân/Doanh nghiệp</label>
            <div class="col-md-12">
                <input type="text" name="CusName" value="${CusName}" class="form-control" />
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-md-4" data-res="">Tên lệ phí</label>
            <div class="col-md-12">
                <input type="text" name="Extra" value="${Extra}" class="form-control" />
            </div>
        </div>

        <div class="form-group form-group-sm">
            <label class="col-md-4" data-res="">Tên lệ phí</label>
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-7">Tên Phí</div>
                    <div class="col-md-7">Số Tiền</div>
                    <div class="col-md-2">Chức năng</div>
                </div>
            </div>
        </div>
        <div class="form-group form-group-sm" id="formProducts">
            
        </div>
        <div class="form-group form-group-sm">
            <label class="col-md-4" data-res=""></label>
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-14"></div>
                    <div class="col-md-2">
                        <i class="icon icon-plus add-product" style="color: blue;"></i>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-md-4" data-res="">Số tiền</label>
            <div class="col-md-12">
                <input type="text" name="" value="" class="form-control amount" />
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-md-4" data-res="">Số tiền bằng chữ</label>
            <div class="col-md-12">
                <input type="text" name="" value="" class="form-control amountinwords" readonly />
            </div>
        </div>
    </div>
</form>