using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Bkav.eGovCloud.Entities.Customer
{
    /// <summary>
    /// Bảng lưu trữ thông tin của client
    /// </summary>
    public class Client
    {
        /// <summary>
        /// Lấy hoặc thiết lập Id
        /// </summary>
        public int Id { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập Identifier
        /// </summary>
        public string Identifier { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập Secret
        /// </summary>
        public string Secret { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập Name
        /// </summary>
        public string Name { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập Domain
        /// </summary>
        public string Domain { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập Ip
        /// </summary>
        public string Ip { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập IsActivated
        /// </summary>
        public bool IsActivated { get; set; }
    }
}
