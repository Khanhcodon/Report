using System;
using System.Web;

namespace Bkav.eGovCloud.Entities.Customer
{
    /// <summary>
    /// Bkav Corp. - BSO - eGov - eOffice team
    /// Project: eGov Cloud v1.0
    /// Class : Document - public - Entity
    /// Access Modifiers: 
    /// Create Date : 17082013
    /// Author      : DungHV
    /// Description : Entity tương ứng với bảng Infomation trong CSDL: lưu thông tin các cơ quan
    /// </summary>
    [Serializable]
    public class Infomation
    {
        /// <summary>
        /// Lấy hoặc thiết lập key
        /// </summary>
        public int InfomationId { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập tên cơ quan 
        /// </summary>
        public string Name { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập cơ quan chủ quản
        /// </summary>
        public string ParentName { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập địa chỉ cơ quan 
        /// </summary>
        public string Address { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập địa chỉ thư điện tử cơ quan 
        /// </summary>
        public string Email { get; set; }
        
        /// <summary>
        /// Lấy hoặc thiết lập số điện thoại cơ quan 
        /// </summary>
        public string Phone { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập số mở rộng của Phone
        /// </summary>
        public string PhoneExt { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập số Fax của cơ quan 
        /// </summary>
        public string Fax { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập số website của cơ quan 
        /// </summary>
        public string Website { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập số tên hình thức của cơ quan 
        /// </summary>
        public string Alias { get; set; }

        /// <summary>
        /// Lấy hoặc thiết lập tên hình thức của cơ quan 
        /// </summary>
        public string SystemName { get; set; }
        /// <summary>
        /// Lấy hoặc thiết lập tên hình thức của cơ quan 
        /// </summary>
        public bool IsDisplaySystemName { get; set; }
        /// <summary>
        /// Lấy hoặc thiết lập logo của cơ quan 
        /// </summary>
        public string ImagePath { get; set; }
    
    }
}
