<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Ghi Nhận Vi Phạm</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="../Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="../Content/bkav.egov/egov.home.css" rel="stylesheet" />
    <link href="../Content/bootstrap/css/bootstrap-theme.css" rel="stylesheet" />
    <link href="../Content/bkav.egov/rateemployee/body.css" rel="stylesheet" />
    <link href="../Content/themes/base/jquery.ui.all.css" rel="stylesheet" />
    <link href="../Content/bkav.egov/rateemployee/tautocomplete.css" rel="stylesheet" />

    <script src="../Scripts/bkav.egov/libs/jquery/jquery-2.2.3.min.js"></script>
    <script src="../Scripts/bkav.egov/libs/jquery/jquery-ui-1.11.4.custom/jquery-ui.min.js"></script>
    <script src="../Scripts/bkav.egov/libs/jquery/jquery.layout-latest.min.js"></script>

    <script src="../Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="../Scripts/bkav.egov/rateemployee/moment.min.js"></script>
    <script src="../Scripts/bkav.egov/libs/jquery/jQuery.tmpl.min.js"></script>

    <link href="../Scripts/bkav.egov/libs/vakata-jstree-c0cd19c/dist/themes/default/style.min.css" rel="stylesheet" />
    <script src="../Scripts/bkav.egov/libs/vakata-jstree-c0cd19c/dist/jstree.min.js"></script>
    <script src="../Scripts/bkav.egov/rateemployee/colResizable-1.5.min.js"></script>

    <script src="../Scripts/bkav.egov/rateemployee/tautocomplete.min.js"></script>
    <script src="../Scripts/bkav.egov/libs/json2.min.js"></script>
    <script src="../Scripts/bkav.egov/libs/underscore/underscore-1.8.3.min.js"></script>
    <script src="../Scripts/bkav.egov/libs/backbone/backbone-1.3.3.min.js"></script>
    <script src="../Scripts/bkav.egov/rateemployee/colResizable-1.5.min.js"></script>
    <script src="../Scripts/bkav.egov/libs/bootbox.min.js"></script>
    <script src="../Scripts/bkav.egov/rateemployee/bkav.egov.server.min.js"></script>
    @RenderSection("script", required: false)
</head>
<body>
    <div class="full-height ui-layout-container" id="bodycriteria">
        <div class="pane ui-layout-west" style=" border-right:solid 1px #eee; height:100vh;">
            <div class="full-height tree-view">
                <div class="full-height tree" id="trees" tabindex="5000">
                    <nav role="navigation" class="system-tree center-menu">
                        <ul class="nav nav-pills nav-stacked" id="menu-document">
                            <li id="group_1">
                                <a class="list-group-item" data-toggle="collapse" data-parent="#menu-document" data-target="#child1" href="#" style=" display:none">
                                    <span class="icon-paste2 icon menu-icon"></span>
                                    <span class="node-name">Bùi Văn Tiến</span>
                                    <span class="badge reload qtooltip icon-spinner6" data-restitle="egov.resources.index.reload" title="Đồng bộ"></span>
                                </a>
                                <ul id="child1" data-id="1" class="nav panel-collapse collapse in" role="menu">
                                    <li id="3" data-parentid="0">
                                        <a href="/EvaluationCriteria/Index" class="list-group-item wraptext @ViewBag.IconList" style="" data-open="false" data-params="[]">
                                            <span class="badge qtooltip totalUnread" data-totalunread="0" data-total="0" data-hasqtip="3"  title=""></span>
                                            <span class="folder-icon"></span>
                                            <span class="node-name">Danh sách vi phạm</span>
                                        </a>
                                    </li>
                                    <li id="1" data-parentid="0">
                                        <a href="/EvaluationCriteria/CreateCriteria" class="list-group-item wraptext @ViewBag.Criteria" style="" data-open="false" data-params="[]">
                                            <span class="badge qtooltip totalUnread" data-totalunread="0" data-total="0" data-hasqtip="3" title=""></span>
                                            <span class="folder-icon"></span>
                                            <span class="node-name">Quản lý các tiêu chí</span>
                                        </a>
                                    </li>
                                    <li id="2" data-parentid="0">
                                        <a href="/EvaluationCriteria/CheckCriteria" class="list-group-item wraptext @ViewBag.Check" style="" data-open="false" data-params="[]">
                                            <span class="badge qtooltip totalUnread" data-totalunread="0" data-total="0" data-hasqtip="3" title=""></span>
                                            <span class="folder-icon"></span>
                                            <span class="node-name">Đánh giá thi đua</span>
                                        </a>
                                    </li>
                                </ul>
                            </li>

                        </ul>
                    </nav>
                </div>
            </div>
            @*<div id="menuRight" style="padding-top:20px">
                    <ul>
                        <li style=" padding-top:15px;">
                            <span style="color: rgb(0, 130, 153);" class="@ViewBag.IconList"></span>
                            <a href="/EvaluationCriteria/Index" class="active">Danh sách vi phạm</a>
                        </li>
                        <li style=" padding-top:15px;">
                            <span style="color: rgb(0, 130, 153);" class="@ViewBag.Criteria"></span>
                            <a href="/EvaluationCriteria/CreateCriteria">Quản lý các tiêu chí</a>
                        </li>
                        <li style=" padding-top: 15px;">
                            <span style="color: rgb(0, 130, 153);" class="@ViewBag.Check"></span>
                            <a href="/EvaluationCriteria/CheckCriteria">Đánh dấu vi phạm</a>
                        </li>
                    </ul>
                </div>*@
        </div>
        <div class="ui-layout-center pane" style="overflow:auto">
            @RenderBody()
        </div>
        <div id="processing"></div>
    </div>
    @RenderSection("script_relate", required: false)
    <script src="../Scripts/bkav.egov/dev/egov.events.min.js"></script>
    <script src="../Scripts/bkav.egov/dev/egov.status.min.js"></script>
    <script src="../Scripts/bkav.egov/dev/egov.pubsub.min.js"></script>
    <script>
        $(function () {
            $("#bodycriteria").layout({
                resizable: true,
                closable: false,
            });
            $.datepicker.regional["vi-VN"] =
               {
                   closeText: "Đóng",
                   prevText: "Trước",
                   nextText: "Sau",
                   currentText: "Hôm nay",
                   monthNames: ["Tháng một", "Tháng hai", "Tháng ba", "Tháng tư", "Tháng năm", "Tháng sáu", "Tháng bảy", "Tháng tám", "Tháng chín", "Tháng mười", "Tháng mười một", "Tháng mười hai"],
                   monthNamesShort: ["Một", "Hai", "Ba", "Bốn", "Năm", "Sáu", "Bảy", "Tám", "Chín", "Mười", "Mười một", "Mười hai"],
                   dayNames: ["Chủ nhật", "Thứ hai", "Thứ ba", "Thứ tư", "Thứ năm", "Thứ sáu", "Thứ bảy"],
                   dayNamesShort: ["CN", "Hai", "Ba", "Tư", "Năm", "Sáu", "Bảy"],
                   dayNamesMin: ["CN", "T2", "T3", "T4", "T5", "T6", "T7"],
                   weekHeader: "Tuần",
                   dateFormat: "dd/mm/yy",
                   firstDay: 1,
                   isRTL: false,
                   showMonthAfterYear: false,
                   yearSuffix: ""
               };

            $.datepicker.setDefaults($.datepicker.regional["vi-VN"]);
            $('#processing').status({
                subscribe: egov.pubsub.subscribe
            });
        });
    </script>
</body>
</html>
