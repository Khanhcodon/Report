<div class="dg-form">
    <div class="user-dept-popup box">
        <div class="pane-header ui-state-active">
            <span data-res="egov.resources.transfer.searchDgLabel"></span>
                <input type="text" id="searchDg" style="width: 97%; margin: 2px; height: 18px;" />
        </div>
        <div class="user-dept-popup" style="padding: 0;">
            <div id="userDeptPopup" style="background: #FFFFFF">
            </div>
        </div>
    </div>
    <div class="jobtitle-for-dept box">
        <div class="box1" style="padding: 0; height: 50%">
            <div class="pane-header ui-state-active" style="border: 1px solid #aaa; border-left: 0; border-right: 0; padding-left: 6px;">
                <input type="checkbox" id="allJobTitlesForDept" />
                <span data-res="egov.resources.transfer.allJobTitlesForDept"></span>
            </div>
            <div class="box2" style="padding: 0; border-left: 0">
                <table class="table-main" id="tblJobtitlesForDept">
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="box1" style="padding: 0">
            <div class="pane-header ui-state-active" data-res="egov.resources.transfer.jobtitlesDeptPopup">
                Chức danh thuộc phòng ban(đơn vị)
            </div>
            <div id="jobtitlesDeptPopup" class="box2" style="padding: 0; background: #FFFFFF">
            </div>
        </div>
    </div>
    <div class="jobtitle-for-all box">
        <div class="pane-header ui-state-active" style="padding: 0;">
            <div style="padding: 5px 10px" data-res="egov.resources.transfer.jobtitleForAll">
                Cấp nhận bản sao(để biết)
            </div>
            <div style="border-top: 1px solid #aaa; padding: 5px;">
                <input type="checkbox" id="allJobTitles" />
                <span data-res="egov.resources.transfer.allJobTitles"></span>
            </div>
        </div>
        <div class="tbl-jobtitle-for-all" style="padding: 0; height: 73%">
            <table class="table-main" id="tblJobtitlesForAll">
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
    <div class="dialog-south-south ui-widget-content ui-corner-all" style="float: left; width: 100%; margin-top: 5px;">
        <div class="ui-layout-content ui-widget-content ui-corner-all" style="padding: 0;">
            <input type="radio" name="tbAndDxl" id="IsThongbao" checked="checked" /><span data-res="egov.resources.transfer.IsThongbao"></span>
                <input type="radio" name="tbAndDxl" id="IsDxl" /><span data-res="egov.resources.transfer.IsDxl"></span>
                <input type="checkbox" id="IsAttachYk" name="IsAttachYk"/><span data-res="egov.resources.transfer.IsAttachYk"></span>
        </div>
    </div>
</div>
