@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <style type="text/css">
        html, body {
            height: 100%;
            overflow: hidden;
        }

        iframe {
            /* display: none; */
            width: 100%;
            height: 100%;
            border: none;
        }
    </style>
    <script type="text/javascript">
        function print(url) {
            var iframe = this._printIframe;
            if (!this._printIframe) {
                iframe = this._printIframe = document.getElementById('content');
                iframe.focus();
                iframe.contentWindow.print();
            }
        }

    </script>

</head>
<body>
    <iframe src="@ViewBag.FilePath" onload="print()" id="content"></iframe>

