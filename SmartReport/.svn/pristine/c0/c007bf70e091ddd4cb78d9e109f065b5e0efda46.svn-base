.mchk-lbl {
    position: relative;
    display: block;
    height: 20px;
    width: 44px;
    background: #C5C5C5;
    border-radius: 100px;
    cursor: pointer;
    -moz-transition: all 0.3s ease;
    -o-transition: all 0.3s ease;
    -webkit-transition: all 0.3s ease;
    transition: all 0.3s ease;
}

    .mchk-lbl::after {
        position: absolute;
        left: -2px;
        top: -3px;
        display: block;
        width: 26px;
        height: 26px;
        border-radius: 100px;
        background: #9EA6FF;
        box-shadow: 0 3px 3px rgba(0,0,0,0.05);
        content: '';
        -moz-transition: all 0.3s ease;
        -o-transition: all 0.3s ease;
        -webkit-transition: all 0.3s ease;
        transition: all 0.3s ease;
    }

    .mchk-lbl:active:after {
        -moz-transform: scale(1.15, 0.85);
        -ms-transform: scale(1.15, 0.85);
        -o-transform: scale(1.15, 0.85);
        -webkit-transform: scale(1.15, 0.85);
        transform: scale(1.15, 0.85);
    }

.mchk:checked ~ label {
    background: rgba(63, 81, 181, 0.79);
}

    .mchk:checked ~ label:after {
        left: 20px;
        background: #283996;
    }

