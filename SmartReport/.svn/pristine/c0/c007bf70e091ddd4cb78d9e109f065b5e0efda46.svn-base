.form-control {
    background: none;
    height: 42px;
    border-radius: 0 !important;
    padding: 8px 0;
    border: none;
    border-bottom: 1px solid #e1e2e2;
    box-shadow: none;
}

    .form-control:focus {
        box-shadow: none;
        border-bottom: none;
    }

/*#region Form label */
.form-label {
    color: #757575;
    position: absolute;
    top: 15px;
    left: 5px;
    -moz-transition: all ease-out .2s;
    -o-transition: all ease-out .2s;
    -webkit-transition: all ease-out .2s;
    transition: all ease-out .2s;
}

.form-control:focus ~ .form-label,
.form-control:valid ~ .form-label,
.form-control.active ~ .form-label {
    top: -15px;
    font-size: 15px !important;
    color: rgb(0, 46, 153);
}

/*#region bars */
.form-control-bar {
    position: relative;
    display: block;
    width: 100%;
    height: 2px;
}

    .form-control-bar:before,
    .form-control-bar:after {
        content: '';
        height: 2px;
        width: 0;
        bottom: 1px;
        position: absolute;
        background: #3f51b5;
        -moz-transition: 0.2s ease all;
        -webkit-transition: 0.2s ease all;
        -o-transition: 0.2s ease all;
        transition: 0.2s ease all;
    }

    .form-control-bar:before {
        left: 50%;
    }

.form-group .form-control-bar:after {
    right: 50%;
}

.form-control:focus ~ .form-control-bar:before,
.form-control:focus ~ .form-control-bar:after {
    width: 50%;
}
/*#region HightLight */
.form-control-hightlight {
    position: absolute;
    height: 60%;
    width: 50%;
    top: 25%;
    left: 0;
    pointer-events: none;
    opacity: 0.5;
}

/* active state */
.form-control:focus ~ .form-control-hightlight {
    -webkit-animation: inputHighlighter 0.3s ease-out;
    -moz-animation: inputHighlighter 0.3s ease-out;
    -o-animation: inputHighlighter 0.3s ease-out;
    animation: inputHighlighter 0.3s ease-out;
}

/* ANIMATIONS ================ */
@-webkit-keyframes inputHighlighter {
    from {
        background: #3f51b5;
    }

    to {
        width: 0;
        background: transparent;
    }
}

@-moz-keyframes inputHighlighter {
    from {
        background: #3f51b5;
    }

    to {
        width: 0;
        background: transparent;
    }
}

@keyframes inputHighlighter {
    from {
        background: #3f51b5;
    }

    to {
        width: 0;
        background: transparent;
    }
}

