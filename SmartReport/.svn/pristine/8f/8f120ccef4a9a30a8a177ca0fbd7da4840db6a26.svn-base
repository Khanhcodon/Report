<script>
    egov.common.targetForComments = [];

    /*Dữ liệu load lên form*/
    // Danh sách Người dùng - Chức danh - Phòng ban
    egov.database.allUserDepartments = @(string.IsNullOrEmpty(ViewBag.AllUserDepartments) ? MvcHtmlString.Create("[]") : Html.Raw(ViewBag.AllUserDepartments));
