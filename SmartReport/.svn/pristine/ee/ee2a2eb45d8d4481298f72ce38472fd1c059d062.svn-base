#toolbar, .toolbar
{
    z-index: 1;
    filter: none !important;
    border-bottom: 1px solid white;
}

.toolbar
{
    height: 35px;
}

.toolbar-item
{
    margin: 0;
    padding: 0;
    list-style-position: outside;
    list-style-type: none;
    vertical-align: middle;
}

.toolbar-item .toolbar-icon
{
    width: 24px;
    height: 24px;
    background-position: 1px 1px;
}

.toolbar .divider
{
    display: inline-block;
    margin-left: 21px;
    width: 2px;
    height: 28px;
    background: url("/content/images/toolbar/common/freshsprites-32x32-dark-ltr-93463.png") no-repeat scroll 50% -1015px transparent;
    vertical-align: middle;
}

/*.toolbar .disabled
{
    opacity: 0.35;
}*/

.toolbar .toolbar-btn
{
    margin-left: 12px;
    height: 28px;
    border: 0 none;
    background-color: transparent;
    background-image: url("/content/images/toolbar/common/toolbar.png");
    background-repeat: no-repeat;
    box-shadow: none;
    padding-left: 13px;
    line-height: 28px;
}

.toolbar .toolbar-btn a
{
    display: block;
    padding: 3px 0 0 20px;
    height: 20px;
    outline: 0 none;
    background-color: transparent;
    color: #454545;
    text-decoration: none;
    text-shadow: 1px 1px 1px rgba(255, 255, 255, 0.5);
    font-weight: bold;
    font-size: 12px;
}

.toolbar .toolbar-btn a b
{
    display: inline-block;
    width: 12px;
    height: 7px;
    background: url("/content/images/toolbar/common/freshsprites-16x16-ltr-98433.png") no-repeat scroll 0 0 transparent;
    background-position: 0 -548px;
    text-indent: -9999px;
}

.toolbar .toolbar-outer
{
    display: table-cell;
    margin-left: 5px;
    padding-top: 4px;
}

.toolbar .btn:hover
{
    background-position: 0 -32px;
    color: #333333;
    text-decoration: none;
    -moz-transition: none;
    -o-transition: none;
    -webkit-transition: none;
    transition: none;
}

