DROP TABLE IF EXISTS `documentrelated`;
CREATE TABLE `documentrelated`  (
  `DocumentRelatedId` int(11) NOT NULL AUTO_INCREMENT,
  `Name` varchar(500) CHARACTER SET utf8 COLLATE utf8_unicode_ci NULL DEFAULT NULL,
  `Description` varchar(500) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `EmbryonicLocationName` varchar(500) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `EmbryonicPath` varchar(500) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `CreatedAt` datetime(0) NOT NULL,
  `CreatedBy` int(11) NOT NULL,
  `UpdatedAt` datetime(0) NULL DEFAULT NULL,
  `UpdatedBy` int(11) NULL DEFAULT NULL,
  PRIMARY KEY (`DocumentRelatedId`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8 COLLATE = utf8_unicode_ci ROW_FORMAT = Dynamic;