.clock-circle {
    width: 50px;
    height: 45px;
    margin: 0 auto;
    position: relative;
}

.clock-face {
    width: 100%;
    height: 100%;
}

.clock-hour {
    width: 0;
    height: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: 0px 0px -4px -26%;
    padding: 0px 0 2px 25%;
    background: #E0C0C0;
    -webkit-transform-origin: 100% 50%;
    -ms-transform-origin: 100% 50%;
    transform-origin: 100% 50%;
    border-radius: 4px 0 0 4px;
}

.clock-minute {
    width: 0;
    height: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -31% -2px 0;
    padding: 31% 1px 0px;
    background: #E0C0C0;
    -webkit-transform-origin: 50% 100%;
    -ms-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
    border-radius: 3px 3px 0 0;
}

.clock-second {
    width: 0;
    height: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -37% -1px 0px -2px;
    padding: 37% 1px 0;
    background: #B2813F;
    -webkit-transform-origin: 50% 100%;
    -ms-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
}

.clock-hour-3, .clock-hour-6, .clock-hour-9, .clock-hour-12 {
    position: absolute;
    /*background: rgb(128, 128, 128);*/
    font-size: 7px;
    padding: 0;
    margin: 0;
    line-height: 10px;
    height: 3px;
    width: 3px;
    border-radius: 50%;
}

.clock-hour-3 {
    right: 2px;
    top: 21px;
}

.clock-hour-6 {
    bottom: 2px;
    left: 22.5px;
}

.clock-hour-9 {
    left: 2px;
    top: 21px;
}

.clock-hour-12 {
    top: 2px;
    left: 22.5px;
}

