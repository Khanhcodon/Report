.mdl-textfield{width:100%;padding:20px 0}.mdl-textfield__input{height:42px;text-indent:15px}.mdl-textfield__label:after{bottom:16px}.field-validation-error{position:absolute}.mdl-textfield--floating-label.is-focused .mdl-textfield__label,.mdl-textfield--floating-label.is-dirty .mdl-textfield__label{top:0}form{padding:10px 15px}.header{padding:0 30px}.header span{font-size:30px;font-weight:bold}#bkavlabel{color:#ff7b00}#slash{color:#aeaeae}#egovlabel{color:#fff}.imgBanner{float:right;width:100%}#login{background:#3f51b5;color:#fff}.login-processing{position:absolute;width:100%;height:100%;height:-webkit-calc(100% - 56px);height:-moz-calc(100% - 56px);height:calc(100% - 56px);top:56px;background:#fff;z-index:100;display:none}.login-processing .wrap-spinner{margin-top:35%}.page-content{max-width:700px;margin:0 auto}#error{padding:0 15px;margin-top:10px;text-indent:15px}#captchaWrap{display:none;text-align:center}#captchaWrap a{display:inline-block;background:#d7d7d7;border:none;border-radius:2px;height:36px;line-height:36px;min-width:64px;padding:0 16px;font-weight:500;text-transform:uppercase;text-decoration:none;margin:10px}#captchaWrap input{height:42px;text-indent:15px;border:none;border-bottom:1px solid rgba(0,0,0,.34);display:block;font-size:16px;margin:0;padding:4px 0;width:100%;background:none;text-align:left}@media(min-width:768px){.page-content .login{margin-top:100px}.header{padding:0 150px}@media(orientation:portrait){.header{padding:0 70px}.imgBanner{width:300px}}}