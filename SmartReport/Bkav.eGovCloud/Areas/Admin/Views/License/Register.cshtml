@model RegisterLicenseModel
@{
    ViewBag.Title = Localizer("Bkav.eGovCloud.Areas.Admin.Title.License.Index");
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="section-header">
    <div class="title">
        <i class="fa icon-file"></i>
        <span data-res="egov.resources.sitemap.license">Đăng ký bản quyền</span>
    </div>
    <div class="options">
        <a class="btn  btn-warning control-btn" id="submit" href="#">
            <span class="icon icon-checkmark"></span>
            <span class="text-uppercase" data-res="egov.resources.buttons.save"></span>
        </a>
    </div>
</div>
<div>
    @Html.Partial("_Notifications")
    <form method="post" action="@Url.Action("RegisterLicenseEo")" class="form-horizontal">
        <div class="form-group">
            <label class="form-label col-sm-2" data-res="egov.resources.license.key">Mã kích hoạt</label>
            <div class="col-sm-14">
                @Html.TextBoxFor(model => model.Key)
            </div>
        </div>
        <div class="form-group">
            <label class="form-label col-sm-2" data-res="egov.resources.license.customername">Khách hàng</label>
            <div class="col-sm-14">
                @Html.TextBoxFor(model => model.CustomerName)
            </div>
        </div>
        <div class="form-group">
            <label class="form-label col-sm-2" data-res="egov.resources.license.Phone">Số điện thoại</label>
            <div class="col-sm-14">
                @Html.TextBoxFor(model => model.Phone)
            </div>
        </div>
        <div class="form-group">
            <label class="form-label col-sm-2" data-res="egov.resources.license.Email">Email</label>
            <div class="col-sm-14">
                @Html.TextBoxFor(model => model.Email)
            </div>
        </div>
    </form>
</div>