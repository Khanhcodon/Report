@model Bkav.eGovCloud.Areas.Admin.Models.DomainModel
@{
    ViewBag.Title = Localizer("Bkav.eGovCloud.Areas.Admin.Title.ConnectionSetting.General");
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<script src="@Url.Content("~/Scripts/bkav.egov/libs/jquery/jquery.validate.min.js")"></script>
<script src="@Url.Content("~/Scripts/bkav.egov/libs/jquery/jquery.validate.unobtrusive.js")"></script>
<script src="@Url.Content("~/Scripts/bkav.egov/libs/jquery/jQuery-tmpl.modified.js")"></script>

<div class="section-header">
    <div class="title">
        <i class="fa icon-archive"></i>
        <span>@Localizer("Setting.ConnectionSetting.Tab.Title")</span>
    </div>
    <div class="options">
        <a class="btn  btn-warning control-btn" id="submit" href="#">
            <span class="icon icon-checkmark"></span>
            <span class="text-uppercase" data-res="egov.resources.buttons.save"></span>
        </a>
        <a class="btn  btn-default control-btn" href="/admin/domain/Index">
            <span class="icon icon-arrow-left2"></span>
            <span class="text-uppercase" data-res="egov.resources.buttons.back"></span>
        </a>
    </div>
</div>
@using (Html.BeginForm())
{
    <div id="tabs" class="bkavegov">
        @Html.Partial("_Notifications")
        @Html.Partial("_CreateOrEdit", Model)
    </div>
}