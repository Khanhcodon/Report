@model Bkav.eGovCloud.Models.DocumentModel
@{
    Layout = null;
    Html.RenderPartial("../Home/_Tokens");
    var lang = Bkav.eGovCloud.Helper.LangHelper.GetUserLangCode();
}
<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="../../Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="../../Content/bootstrap/css/bootstrap-theme.css" rel="stylesheet" />
    <link href="../../Content/bkav.egov/egov.document.css" rel="stylesheet" />
    <link href="../../Content/bkav.egov/egov.color.css" rel="stylesheet" />
    <link href="../../Content/bkav.egov/egov.home.css" rel="stylesheet" />

    <link href="../../Scripts/bkav.egov/libs/jquery/jquery.qtip/jquery.qtip.min.css" rel="stylesheet" />
    <link href="../../Scripts/bkav.egov/libs/bkav/editor/css/bmailEditor.css" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/bkav.egov/resource/egov.resources." + @lang + ".js")"></script>

    <script type="text/javascript">
        egov.popUpDocumentModel = @(Html.Raw(Json.Encode(@Model)));
        egov.popUpTemplateCfg = JSON.parse(@(Html.Raw(Json.Encode(ViewBag.template))));
    </script>
</head>
<body>
    <div>
    </div>
    <div class="alert" id="egovStatus"></div>
    <div id="kqklmrForm" style="width: 100%; height: 100%; display: none">
        <iframe style="width: 100%; height: 100%; border: none;"></iframe>
    </div>
    <div id="cbclForm" style="width: 100%; height: 100%; display: none">
        <iframe style="width: 100%; height: 100%; border: none;"></iframe>
    </div>
</body>
</html>
<script src="/Scripts/require.js" data-main="@Url.Content("/Scripts/bkav.egov/main.js?v=" + (long)(DateTime.UtcNow - new DateTime(1970, 1, 1)).TotalMilliseconds)"></script>