<script type="text/x-jquery-tmpl" id="xlvb_overview">
    <tr class="text-center {{if IsParent}}isParent{{/if}}" style="{{if IsParent}}text-align: center; font-weight: bold; color: #4242c5;{{/if}}" data-type="${TypeStatistics}" data-dept="${ParrentName}" data-click="{{if TypeStatistics == 1}}department{{else}}user{{/if}}">
