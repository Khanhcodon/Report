<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <title>@ViewBag.Title</title>

    <link href="@Url.Content("~/Content/jquery.ui.layout.css")" rel="stylesheet" />
    
    <link href="@Url.Content("~/Content/bkav.egov/egov.document.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/tablet/tablet.css")" rel="stylesheet" />

    <script src="@Url.Content("~/Scripts/bkav.egov/libs/jquery/jquery-2.2.3.min.js")"></script>
    <script src="@Url.Content("~/Content/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bkav.egov/libs/jquery/jquery.ui.layout/jquery.layout-latest.js")"></script>
</head>
<body>
    <div id="main" class="container-fluid full-height">
        <div class="first-row navbar navbar-fixed-top">
            <div class="col-sm-5 col-md-4 col-lg-4 mainleft">
                <a href="#" class="btn btn-primary ui-btn-left">Mục tin</a>
                <span>Xử lý văn bản</span>
            </div>
            <div class="col-sm-11 col-md-12 col-lg-12 mainright">

            </div>
        </div>
        <div class="row second-row">
            <div class="col-sm-5 col-md-4 col-lg-4 mainleft">
                <div class="row">
                    <ul class="nav nav-sidebar">
                        <li><a href="#">Văn bản chờ xử lý</a></li>
                        <li><a href="#">Văn bản đang dự thảo</a></li>
                        <li><a href="#">Văn bản đang theo dõi</a></li>
                        <li><a href="#">Văn bản thông báo</a></li>
                        <li><a href="#">Văn bản mới kết thúc</a></li>
                        <li><a href="#">Văn bản được ủy quyền xử lý</a></li>
                        <li><a href="#">Văn bản ủy quyền vừa xử lý</a></li>
                        <li><a href="#">Văn bản chờ phát hành</a></li>
                    </ul>
                </div>
            </div>
            <div class="col-sm-11 col-md-12 col-lg-12 mainright">
                @RenderBody()
            </div>
        </div>
    </div>
    <script>

    </script>
</body>
</html>
